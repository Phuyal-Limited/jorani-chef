<VirtualHost *:80>
        ServerName technoferry.com
        ServerAdmin webmaster@localhost
        DocumentRoot /var/www/html

      <Directory /var/www/html/lms_technoferry>
        AllowOverride All
        Order allow,deny
        Allow from all
      </Directory>

        LogLevel warn
        ErrorLog ${APACHE_LOG_DIR}/error.log
        CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
